            <Image url="@../img-sound/ButtonR.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="ButtonL" fitHeight="80.0" fitWidth="58.0" layoutX="410.0" layoutY="272.0" onMousePressed="#clickButtonL" pickOnBounds="true" preserveRatio="true">
