            <Image url="@../img-sound/ButtonL.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <StackPane fx:id="validIPsPane" layoutX="207.0" layoutY="230.0" prefHeight="189.0" prefWidth="200.0" stylesheets="@style1.css" />
